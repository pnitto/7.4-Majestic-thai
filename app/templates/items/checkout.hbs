<div class="checkout-div">
  <h1 class="order-header">Your Order</h1>
  <h3 class="item-header">Item</h3>
  <h3 class="subtotal">Subtotal</h3><h4 class="subtotal-number">${{subtotal}}</h4>
    <ul class="checkout-item-ul">
    {{#each items}}
    <li class="checkout-item">{{name}}</li>
    <button class="delete-item-btn">X</button>
    {{/each}}
  </ul>
  <input type="submit" class="submit-order-btn">
</div>
